<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bdi.sp.vo.DelMapper">
  <select id="selectDelList" resultType="com.bdi.sp.vo.Del">
    select * from del
       <where>
    	<if test="name!=null"> and name=#{name}</if>
    	<if test="num!=null"> and num=#{num}</if>
    	<if test="age!=null"> and age=#{age}</if>
    </where>
  </select>
   <select id="selectDel" resultType="com.bdi.sp.vo.Del">
    select * from del
     where num=#{num}
  </select>
</mapper>